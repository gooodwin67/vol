{
    "name": "@yandex-games/sdk-dev-proxy",
    "version": "0.0.1",
    "description": "Package for local game development, allows you to proxy requests to sdk",
    "engines": {
        "node": ">=12.0.0"
    },
    "dependencies": {
        "colorette": "2.0.20",
        "express": "4.18.2",
        "express-http-proxy": "1.6.3",
        "node-html-parser": "6.1.12",
        "openurl": "1.1.1",
        "tslib": "2.6.2",
        "typescript": "5.1.6"
    },
    "devDependencies": {
        "@games-frontend/ci-flowgen": "workspace:*",
        "@games-frontend/games-secrets": "workspace:*",
        "@games-frontend/lint": "workspace:*",
        "@types/express": "4.17.21",
        "@types/express-http-proxy": "1.6.3",
        "@types/openurl": "1.0.3",
        "esbuild": "0.19.11",
        "eslint-plugin-games-frontend": "workspace:*",
        "ts-node": "10.9.2"
    },
    "scripts": {
        "build": "esbuild src/index.ts --bundle --outdir=bin --platform=node --target=node12 --minify",
        "prepublishOnly": "pnpm run build",
        "preinstall": "npx only-allow pnpm",
        "install:deps": "pnpm i --filter {.}...",
        "ci:typecheck": "tsc --build",
        "ci:lint": "pnpm eslint --ext .js,.jsx,.ts,.tsx .",
        "lint:fix": "pnpm run ci:lint --fix"
    },
    "bin": "./bin/index.js",
    "files": [
        "src",
        "bin"
    ]
}
